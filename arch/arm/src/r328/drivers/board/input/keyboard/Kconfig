menuconfig DRIVERS_KEYBOARD
	default n
	bool "keyboard driver"

if DRIVERS_KEYBOARD

config DRIVERS_R328_KEYBOARD
	bool "enable lradc keyboard driver"
	default n

config DRIVERS_R328_GPADC_KEY
	bool "enable gpadc keyboard driver"
	default n

config DRIVERS_R328_GPIO_KEY
	bool "enable gpio keyboard driver"
	default n
endif
